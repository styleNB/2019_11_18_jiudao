/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

@font-face {
  font-family: 'iconfont';  /* project id 1438355 */
  src: url('//at.alicdn.com/t/font_1438355_zixwkkoknmi.eot');
  src: url('//at.alicdn.com/t/font_1438355_zixwkkoknmi.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_1438355_zixwkkoknmi.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1438355_zixwkkoknmi.woff') format('woff'),
  url('//at.alicdn.com/t/font_1438355_zixwkkoknmi.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1438355_zixwkkoknmi.svg#iconfont') format('svg');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-icon--:before {
  content: "\e79e";
}
.icon-xihuanlike:before {
  content: "\e622";
}